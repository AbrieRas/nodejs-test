{
    "builds": [
        {
            "src": "./QuickStepByStepDbConn.js",
            "use": "@vercel/node"
        },
        {
            "src": "./home.html",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
          "src": "/(.*)",
          "dest": "QuickStepByStepDbConn.js"
        },
        {
          "src": "/(.*)",
          "headers": {
              "cache-control": "s-maxage=1000"
          },
          "dest": "/home.html"
        }
    ]
  }
